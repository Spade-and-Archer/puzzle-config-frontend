{
    "env": {
        "browser": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "react/prop-types" : "off",
        "no-unused-vars": "warn"
    }
}
